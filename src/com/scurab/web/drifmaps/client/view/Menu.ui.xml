<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="com.scurab.web.drifmaps.language.Words" field="res" />
	<g:HTMLPanel>
		<g:Button text="{res.Add}" styleName="button add" ui:field="btnAdd"/>
		<g:Button text="{res.Save}" styleName="button save" ui:field="btnSave"/>
		<g:Button text="{res.Star}" styleName="button star" ui:field="btnStar"/>
		<g:DisclosurePanel width="100%" height="100%" animationEnabled="true" ui:field="menuContentHolder">
			<g:header></g:header>
		</g:DisclosurePanel>
	</g:HTMLPanel>
</ui:UiBinder> 