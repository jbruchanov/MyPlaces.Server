<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.mapContainer
		{
			background-color:#DCDCDC;
			left:0px;
			right:0px;
			top:0px;
			bottom:0px;
		}
		.view
		{
			position: absolute;
			left:0px;
			right:0px;
			top:0px;
			bottom:0px;

		}
	
	</ui:style>
	<g:HTMLPanel width="100%" height="" styleName="{style.view}">
		<g:DockLayoutPanel unit="EM" width="100%" height="100%">
			<g:north size="7.7">
				<g:HTMLPanel/>
			</g:north>
			<g:west size="7.7">
				<g:HTMLPanel>
					<g:Button ui:field="btnShowMap" text="Map" width="100%"/>
					<g:Button ui:field="btnTest" text="Test" width="100%"/>
				</g:HTMLPanel>
			</g:west>
			<g:center>
				<g:HTMLPanel ui:field="contentHolder" width="100%" height="100%">
					<div class="mapContainer" id="mapContainer">
					</div>
				</g:HTMLPanel>
			</g:center>
		</g:DockLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 